create table EmployeeDetail(EmployeeID int primary key identity, FirstName varchar(50) Not null, LastName varchar(50) Not null,
Salary bigint Not null, JoiningDate datetime Not null, Department varchar(20) Not null,Gender varchar(20) Not null);
